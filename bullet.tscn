func _ready():
	# Called every time the node is added to the scene.
	# Initialization here
	set_process(true)
	
func get_speed():
	return speed

func set_speed(speed_to_set):
	speed = speed_to_set
	
func _process(delta):
	move(speed * delta)
	if left_initial_hitbox:
		if is_colliding():
			var collider = get_collider()
			
			if null != collider:
				print (\"notified...\")
				collider._notify(1337)
			else:
				print(\"eehhh collider is leider null\")
			
			remove_child(get_node(\"Sprite\"))
			remove_child(get_node(\"CollisionShape2D\"))
			queue_free()
	else:
		if not is_colliding():
			left_initial_hitbox = true"

